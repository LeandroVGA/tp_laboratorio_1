#ifndef EMPLOYEE_H_
#define EMPLOYEE_H_
#define LONG_NAME 51
#define TRUE 1
#define FALSE 0
#define MIN_SECTOR 50
#define MAX_SECTOR 9999


typedef struct
{
	int id;
	char name [LONG_NAME];
	char lastName [LONG_NAME];
	float salary;
	int sector;
	int isEmpty;

}Employee;

int addEmployee(Employee* arrayEmployee, int indice, int id, char name[],char lastName[],float salary,int sector);

#endif /* EMPLOYEE_H_ */
